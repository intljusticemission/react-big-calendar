{
  "presets": ["jason"],
  "plugins": [
    ["transform-react-remove-prop-types", { "mode": "wrap" }]
  ],
  "env": {
    "esm": {
      "presets": [
        ["jason", { "modules": false }]
      ],
      "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        ["babel-plugin-transform-rename-import", {
          replacements: [{ original: "lodash", replacement: "lodash-es" }]
        }]
      ]
    }
  }
}
